PROGRAM tester4
  
DECLARE
	INTEGER z
	INTEGER u	
	INTEGER x
	INTEGER bool
	INTEGER y
	INTEGER t
	STRING colour



	
BEGIN


PRINT "Please type an integer :"
READ z
IF z > 5 THEN 

	PRINT "Please type another integer: "

	READ u	

	IF 1 > u  THEN PRINT "THE NUMBER YOU TYPED IS LESS THAN 1" ENDIF

	PRINT "Please type the number of repetitions: "

	READ x

	WHILE x > 0 DO
		PRINT x
		SET x := x - 1
	ENDWHILE


	PRINT "LAST THEN STATEMENT"
#------------------------------------------------------------------------
	ELSE
#------------------------------------------------------------------------

	PRINT "Please type your favourite colour: "
	READ colour

	PRINT "Please type the number of repetitions: "
	READ t
	
	WHILE t > 0 DO
		PRINT colour
		SET t := t-1
	ENDWHILE	

	PRINT "Do you want to print hey? Type 1 for yes or 0 for no: "
	READ bool
	IF bool = 1 THEN PRINT "Hey" ENDIF
	

	
		
		SET x := 5
		WHILE x > 0 DO
			SET y := 5
			WHILE y > 0 DO
				PRINT "y-loop"
				SET y := y-1
			ENDWHILE	
			PRINT "	X-LOOP"
		

			SET x := x-1
		
		ENDWHILE	

	PRINT "HELLO THERE, ELSE WAS PERFORMED"
	PRINT "LAST ELSE STATEMENT"
ENDIF	



	

















